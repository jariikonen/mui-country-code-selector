{
  "name": "demo-page",
  "version": "0.2.0",
  "private": false,
  "author": "Jari Ikonen",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest run",
    "convert-docs": "tsx docsToHTML && sh docsToHTML/fixLinks.sh",
    "ci-and-build": "npm ci -w demo-page && npm run convert-docs -w demo-page && npm run build -w demo-page",
    "cleanup": "rm -rf ./dist && rm -rf ./node_modules && rm -rf ./public/docs",
    "rebuild-docs": "npm run convert-docs -w demo-page && npm run build -w demo-page"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@fontsource/roboto": "^5.2.5",
    "@mui/icons-material": "^7.1.1",
    "@mui/material": "^7.1.1",
    "match-sorter": "^8.0.2",
    "mui-country-code-selector": "file:../mui-country-code-selector",
    "react-syntax-highlighter": "^15.6.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@types/markdown-it": "^14.1.2",
    "@types/node": "^22.15.29",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.6",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@vitejs/plugin-react": "^4.5.1",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.4.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.2.0",
    "highlight.js": "^11.11.1",
    "husky": "^9.1.7",
    "markdown-it": "^14.1.0",
    "prettier": "3.5.3",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.1",
    "vite": "^6.3.5"
  },
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0 || ^19.0.0",
    "react-dom": "^17.0.0 || ^18.0.0 || ^19.0.0"
  }
}
