{
  "name": "demo-page",
  "version": "0.0.2",
  "private": false,
  "author": "Jari Ikonen",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest run",
    "convert-docs": "tsx docsToHTML && sh docsToHTML/fixLinks.sh",
    "install-and-build": "npm i -w demo-page && npm run convert-docs -w demo-page && npm run build -w demo-page",
    "cleanup": "rm -rf ./dist && rm -rf ./node_modules && rm -rf ./public/docs",
    "rebuild-docs": "npm run convert-docs -w demo-page && npm run build -w demo-page"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@fontsource/roboto": "^5.0.13",
    "@mui/icons-material": "^5.15.17",
    "@mui/material": "^5.15.17",
    "match-sorter": "^6.3.4",
    "mui-country-code-selector": "^0.0.0",
    "react-syntax-highlighter": "^15.5.0"
  },
  "devDependencies": {
    "@types/markdown-it": "^14.1.1",
    "@types/node": "^20.12.11",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "eslint": "^8.56.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "highlight.js": "^11.9.0",
    "markdown-it": "^14.1.0",
    "prettier": "3.2.5",
    "tsx": "^4.10.2",
    "typescript": "^5.4.5",
    "vite": "^5.2.11"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
